# Copy this file to .env and set values for your environment.
# Do not commit .env to version control.

# SupportCopilot AI Virtual Assistant widget URL (optional).
# Leave unset to use default production widget.
# For production HTTPS, use the SupportCopilot widget URL, e.g.:
# REACT_APP_SUPPORTCOPILOT_URL=https://supportcopilot-web-production.up.railway.app/widget/e7cbf11e-6668-4fd4-a751-fba67b899bcc
REACT_APP_SUPPORTCOPILOT_URL=

# Google Analytics 4 Measurement ID for tracking
# Your GA4 ID is already configured in public/index.html
# Current ID: G-B6H259W4M9
REACT_APP_GA_MEASUREMENT_ID=G-B6H259W4M9

# Newsletter API endpoint (if you integrate with a newsletter service)
REACT_APP_NEWSLETTER_API_URL=

# Postgres connection (used by Netlify functions in local dev via netlify dev)
# Set in Netlify dashboard for production.
POSTGRES_CONNECTION_STRING=
